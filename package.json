{
  "name": "next-typescript-chakra-ui-starter",
  "version": "0.1.0",
  "description": "Unofficial Next.js + Typescript + Chakra UI starter with a latest package",
  "author": "Danang Eko Alfianto <github.com/danangekal>",
  "homepage": "https://github.com/danangekal/next-typescript-chakra-ui-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danangekal/next-typescript-chakra-ui-starter.git"
  },
  "bugs": {
    "url": "https://github.com/danangekal/next-typescript-chakra-ui-starter/issues"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "chakra-ui",
    "docker",
    "pwa",
    "eslint",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|css|scss)\" --write",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "framer-motion": "^5.5.5",
    "next": "^12.0.9",
    "next-pwa": "^5.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.37",
    "eslint": "8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
