{
  "name": "next-typescript-chakra-ui-starter",
  "version": "0.1.0",
  "description": "Unofficial Next.js + Typescript + Chakra UI starter with a latest package",
  "author": "Danang Eko Alfianto <github.com/danangekal>",
  "homepage": "https://github.com/danangekal/next-typescript-chakra-ui-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danangekal/next-typescript-chakra-ui-starter.git"
  },
  "bugs": {
    "url": "https://github.com/danangekal/next-typescript-chakra-ui-starter/issues"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "chakra-ui",
    "docker",
    "pwa",
    "eslint",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|css|scss)\" --write",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.4",
    "next": "^13.5.4",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "pinst": "^3.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.14"
  }
}
